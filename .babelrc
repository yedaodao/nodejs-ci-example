{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "6.6"
        }
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "add-module-exports",
    "transform-decorators-legacy"
  ],
  "env": {
    "development": {
      "sourceMaps": "inline"
    }
  }
}

